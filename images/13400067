        categoria2.href = "https://www.uninorte.edu.co/web/grupo-prensa/salud";
        categoria2.textContent = 'Salud';
    }
    else if (categoria2.textContent == 'academia1') {
        categoria2.href = "https://www.uninorte.edu.co/web/grupo-prensa/academia";
